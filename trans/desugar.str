module desugar

imports
	include/WebDSL
	datamodel/trans/desugar
	ui/trans/desugar
	function/trans/desugar
	
strategies
	
	desugar-all = alltd(ImportApplication); topdown(try(datamodel-desugar <+ ui-desugar <+ function-desugar))

  ImportApplication :
  	ModuleDefs(name, import*, section*) -> ModuleDefs(name, [Imports("_application") | import*], section*)
  	
