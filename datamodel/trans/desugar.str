module trans/desugar

imports
	include/WebDSL
	
rules
	datamodel-desugar:
		|[ entity str { ebd* } ]| -> |[ entity str : Entity { ebd* }]|
	
	datamodel-desugar:
		PropertyNoAnno(pn, refmod, type) -> Property(pn, refmod, type, [])
	
	datamodel-desugar:
		DerivedPropertyNoAnno(pn, refmod, type, expr) -> DerivedProperty(pn, refmod, type, [], expr)
		
		