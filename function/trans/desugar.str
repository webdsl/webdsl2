module trans/desugar

imports
	include/WebDSL
	
rules
	function-desugar:
		GlobalFunction(|[ function x( farg* ) : srt { stat* } ]|) ->
			GlobalFunction(|[ static function x( farg* ) : srt { stat* } ]|)
	
	function-desugar:
		|[ if (e) block ]| -> |[ if (e) block else { }]|
	
	function-desugar:
		|[ [ e0 | x : srt in e1 ] ]| -> |[ [ e0 | x : srt in e1 where true ] ]|
	
	function-desugar:
		Void() -> "Void"
	
